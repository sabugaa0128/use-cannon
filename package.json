{
  "name": "use-cannon",
  "version": "0.0.0",
  "description": "cannon hooks for react-three-fiber",
  "keywords": [
    "cannon",
    "three",
    "react",
    "react-three-fiber"
  ],
  "author": "Paul Henschel",
  "license": "MIT",
  "main": "./dist/web.cjs.js",
  "module": "./dist/web.js",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist && rollup",
    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "peerDependencies": {
    "react": ">=16.13",
    "react-dom": ">=16.13",
    "react-three-fiber": ">=4.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/plugin-transform-modules-commonjs": "7.8.3",
    "@babel/plugin-transform-parameters": "7.8.4",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/plugin-transform-template-literals": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "@babel/preset-react": "7.8.3",
    "@babel/register": "^7.8.6",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@surma/rollup-plugin-off-main-thread": "^1.4.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cannon-es": "^0.6.3",
    "husky": "^4.2.3",
    "lebab": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-bundle-worker": "^0.1.0",
    "rollup-plugin-cjs-es": "^0.9.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-web-worker-loader": "^0.9.0",
    "three": "^0.114.0"
  }
}
