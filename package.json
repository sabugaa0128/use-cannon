{
  "name": "@pmndrs/use-cannon",
  "description": "monorepo for @pmndrs/use-cannon",
  "keywords": [
    "cannon",
    "three",
    "worker",
    "physics",
    "react",
    "react-three-fiber"
  ],
  "author": "Paul Henschel",
  "contributors": [
    "Cody Persinger <codypersinger@gmail.com> (https://github.com/codynova)",
    "Bjorn Stromberg <bjorn@bjornstar.com> (https://github.com/bjornstar)"
  ],
  "repository": "github:pmndrs/use-cannon",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn workspace @react-three/cannon run build",
    "eslint": "yarn workspaces foreach -v run eslint",
    "eslint-fix": "yarn workspaces foreach -v run eslint-fix",
    "prettier": "yarn workspaces foreach -v run prettier",
    "prettier-fix": "yarn workspaces foreach -v run prettier-fix",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.2.0",
  "workspaces": [
    "packages/cannon-worker-api",
    "packages/react-three-cannon",
    "packages/react-three-cannon-examples"
  ],
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,md}": "prettier --write"
  }
}
